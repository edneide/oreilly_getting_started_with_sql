SELECT * FROM STATION_DATA; 

-- INSTRUÇÃO WHERE ----------
SELECT * FROM STATION_DATA
WHERE YEAR = 2010;

-- TUDO MENOS 2010 ---------
SELECT * FROM STATION_DATA
WHERE YEAR != 2010;

-- OU
SELECT * FROM STATION_DATA
WHERE YEAR <> 2010;

-- BETWEEN ----------
SELECT * FROM STATION_DATA
WHERE YEAR BETWEEN 2005 AND 2010;

